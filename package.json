  "main": "./lib/main",
  "version": "0.2.1",
  "description": "A Atom package to enable remote pairing",
  "activationCommands": {
    "atom-workspace": "atom-remote-pair:toggle"
  },
  "contributors": [
    {
      "name": "Leon Maia",
      "email": "hi@leonmaia.com"
    },
    {
      "name": "Luiz Filho",
      "email": "luizbafilho@gmail.com"
    }
  ],
  "repository": "https://github.com/atom/atom-remote-pair",
  "license": "MIT",
  "engines": {
